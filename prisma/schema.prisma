datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Info {
  id         Int      @id @default(autoincrement())
  times      Int
  canvas     Canvas[] @relation("canvas")
  updated_at DateTime @updatedAt
}

model Canvas {
  id         Int      @id @default(autoincrement())
  row        Int      @unique
  dots       String[]
  updated_at DateTime @updatedAt
  info       Info?    @relation(name: "canvas", fields: [infoId], references: [id])
  infoId     Int?
}
